{
  "name": "miaoxing/services",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Extra Services",
  "authors": [
    {
      "name": "twinh",
      "email": "twinhuang@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Miaoxing\\Services\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Services\\": "tests"
    }
  },
  "require": {
    "miaoxing/plugin": "^0.13.2",
    "laravel/framework": "^7.24",
    "phpoffice/phpspreadsheet": "^1.19"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.13.x-dev",
      "wei/wei": "twinh/wei as 0.15.x-dev"
    }
  },
  "suggest": {
    "miaoxing/queue": "Required to use Laravel queue"
  }
}
