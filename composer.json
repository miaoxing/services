{
  "name": "miaoxing/services",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Extra Services",
  "authors": [
    {
      "name": "twinh",
      "email": "twinhuang@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Miaoxing\\Services\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Services\\": "tests"
    }
  },
  "require": {
    "miaoxing/plugin": "^0.4.7",
    "laravel/framework": "^7.24"
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev"
    }
  },
  "suggest": {
    "miaoxing/queue": "Required to use Laravel queue"
  }
}
